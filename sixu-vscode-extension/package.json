{
  "name": "sixu-vscode-extension",
  "displayName": "SiXu Language Support",
  "description": "SiXu Language Support",
  "version": "0.1.0",
  "main": "./src/extension.mjs",
  "dependencies": {
    "vscode-languageclient": "^10.0.0-next.19"
  },
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "sixu",
        "aliases": [
          "思绪",
          "SiXu",
          "sixu"
        ],
        "extensions": [
          ".sixu"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "sixu",
        "scopeName": "source.sixu",
        "path": "./syntaxes/sixu.tmLanguage.json"
      }
    ]
  }
}
